{
  "name": "@fabrix/spool-plv8",
  "version": "1.1.0",
  "description": "Spool: require() and execute nodejs code in Postgres with PLV8 and Fabrix",
  "homepage": "http://trailsjs.io",
  "author": {
    "name": "Langa",
    "email": "hello@langa.io",
    "url": "langa.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabrix-app/spool-plv8/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabrix-app/spool-plv8.git"
  },
  "scripts": {
    "build": "tsc -p ./lib/tsconfig.release.json",
    "lint": "tslint -p ./lib",
    "watch": "tsc -w -p ./lib/tsconfig.release.json",
    "test": "npm run clean && npm run lint && npm run build && nyc mocha",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "ci": "cd .. && ci"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "pre-commit": [
    "test"
  ],
  "keywords": [
    "trails",
    "trailpack",
    "trails",
    "trailjs",
    "postgres",
    "plv8",
    "require",
    "graphql"
  ],
  "dependencies": {
    "plv8": "^2.0.2"
  },
  "devDependencies": {
    "lodash": "^4.11.1",
    "@fabrix/fabrix": "^1.1.4",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@fabrix/spool-i18n": "^1.1.0",
    "@fabrix/spool-knex": "^1.1.0",
    "@types/node": "~10.3.4",
    "express": "^4.16.3",
    "pg": "^6.4.2",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "smokesignals": "^3.0.4",
    "supertest": "^3.1.0",
    "tslib": "~1.9.0",
    "tslint": "~5.10.0",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.27.1",
    "typescript": "~2.8.1"
  }
}
